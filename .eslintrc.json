{
  "env": {
    "browser": true,
    "es2024": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "tsx": true,
      "vue": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "vue/no-v-model-argument": "off",
    "no-debugger": "off",
    "no-unused-vars": "off"
  }
}